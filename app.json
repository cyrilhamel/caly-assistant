{
  "expo": {
    "name": "Caly",
    "slug": "caly-assistant",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#6200EE"
    },
    "icon": "./assets/icon.png",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cyrilhamel.caly"
    },
    "android": {
      "package": "com.cyrilhamel.caly",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#6200EE"
      }
    },
    "plugins": [
      "expo-router",
      "expo-notifications"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e2afb114-7ec3-4173-8051-24a4b03fad82"
      }
    }
  }
}
